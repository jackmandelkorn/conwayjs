{
  "manifest_version": 2,

  "name": "ConwayJS",
  "description": "The extension version of ConwayJS.",
  "version": "1.3.2",

  "browser_action": {
   "default_icon": "favicon.png",
   "default_title": "ConwayJS",
   "default_popup": "popup.html"
  },

  "icons":{
    "16": "favicon.png",
    "32": "extIcons/32.png",
    "48": "extIcons/48.png",
    "64": "extIcons/64.png",
    "128": "extIcons/128.png"
  },

  "permissions": [
   "activeTab"
   ]
}
